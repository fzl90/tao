@RequestMapping(value = "/upload_retailer_ids", method = RequestMethod.POST)
    public
    @ResponseBody
    ResponseEntity<?> fileUpload(@RequestParam(value = "file", required = false) MultipartFile multipartFile) throws ServiceException {
        logger.info("上传文件");
        FileUploadResDto fileUploadResDto = new FileUploadResDto();

        String fileUrl = manageAnnounceService.uploadRetailerIdsFile(multipartFile);
        fileUploadResDto.setFileUrl(fileUrl);

        return ResponseEntity.success(fileUploadResDto);
    }